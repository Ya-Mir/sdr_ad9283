window.fbAsyncInit = function(){
  FB.init({
    appId : Drupal.settings.penton_facebook_appid,
    status: false,
    cookie : true,
    oauth : true,
    xfbml : false
  });

/* fb:like */
  FB.Event.subscribe('edge.create', function(href, widget) {
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event35';
    s.events='event35';
    s.eVar33='fb:like';
    s.eVar69=s_account;
    s.tl(this,'o', 'fb:like-' + href);
  });

/* fb:unlike */
  FB.Event.subscribe('edge.remove', function(href, widget) {
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event36';
    s.events='event36';
    s.eVar33='fb:unlike';
    s.eVar69=s_account;
    s.tl(this,'o', 'fb:unlike-' + href);
  });
};
if(Drupal.settings.penton_facebook_appid != "undefined"){
  (function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=" + Drupal.settings.penton_facebook_appid;
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));
}
/* google+ */
function plusoneTrack(obj) {
  var s=s_gi(s_account);
  s.linkTrackVars='eVar33,eVar35,eVar69,events';
  s.linkTrackEvents='event33,event34';
  s.eVar33='Google Plus One Click';
  s.eVar35=obj.state;
  s.eVar69=s_account;
  if(s.eVar35=='on'){s.events='event33';}else{s.events='event34';}
  s.tl(obj,'o','Google Plus One Click');
}

/* twitter */
if(typeof twttr != "undefined"){
  twttr.ready(function(twttr){
    twttr.events.bind('tweet',function(event){
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.eVar33='Twitter Share';
      s.eVar69=s_account;
      s.events=s.linkTrackEvents='event39';
      s.tl(true,'o','Twitter Share');
    });
  });
}

function omnitureSlide(slide) {
    var s = s_gi(s_account);
    s.prop8 = 'Slide ' + slide;
    void(s.t());
}

var newsletters = '',
  userName = '',
  action = '',
  is_comment='',
  is_forum='';

function set_cookie(cname, cvalue, extime) {
  var d = new Date();
  d.setTime(d.getTime() + (extime*1000));
  var expires = "expires="+d.toUTCString();
  document.cookie = cname + "=" + cvalue + "; " + expires + "; path=/";
}

function get_cookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for(var i=0; i<ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0)==' ') c = c.substring(1);
    if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
  }
  return "";
}

function delete_cookie(cname) {
  set_cookie(cname,'',0);
}

function modal_form_loaded(reg_message) {
  var s = s_gi(s_account);
  s.linkTrackVars='prop46,eVar46,eVar69';
  s.eVar46=s.prop46=reg_message;
  s.eVar69=s_account;
  s.tl(this,'o',reg_message);
}

//For Penton Simple Registration forms
function modal_form_loaded_simple(reg_message,need_action) {
  need_action = typeof need_action !== 'undefined' ? need_action : false;
  var s = s_gi(s_account);
  s.linkTrackVars='prop46,eVar46,eVar69';
  if(need_action && action!=''){
    s.linkTrackVars+=',eVar2';
    s.eVar2=action;
    action='';
  }
  newsletters=get_cookie('eVar[1]');
  if(newsletters!=''){
    s.linkTrackVars+=',eVar1';
    s.eVar1=newsletters;
    newsletters='';
    set_newsletters_cookie();
  }
  if(userName!=''){
    s.linkTrackVars+=',eVar10';
    s.eVar10=userName;
  }
  s.eVar46=s.prop46=reg_message;
  s.eVar69=s_account;
  s.tl(this,'o',reg_message);
}

function registration_initiation_action(act_message, new_page) {
  if(act_message==''){
    return;
  }
  new_page=typeof new_page !== 'undefined' ? new_page : false;
  var s = s_gi(s_account);
  s.linkTrackVars='eVar2,eVar69';
  s.eVar2=act_message;
  s.eVar69=s_account;
  if(!new_page){
    s.tl(this,'o',act_message);
  }else{
    set_cookie('eVar[2]',action,3600);
  }
}

jQuery(function($) {
  $(document).ready(function() {
    var login_reg = {      //  'url': 'message',
          '.ctools-modal-register_light': 'light modal registration',
             'a[href^="/user/register"]': 'regular registration',
                '.ctools-modal-register': 'full modal registration',
                'a[href^="/user/login"]': 'regular login',
                  '.ctools-modal-log_in': 'modal login'
        };
    is_forum='';
    is_comment='';
    if($('body').hasClass('node-type-forum')) {
      is_forum='forum ';
      action='forum';
    };
    $.each(login_reg,function(key,value) {
      $(key + ':not(.is-processed)').addClass('is-processed').click(function () {
        $this = $(this);
        if($this.hasClass('login-to-comment')||($this.parent().parent().hasClass('comment_forbidden')||$this.parent().hasClass('no-posting-permission'))){
          is_comment=is_forum+'comment ';
          if(action!='forum'){
            action='comment';
          }
        }else{
          is_comment='';

        };
        //modal_form_loaded(is_comment+value);
      });
    });
    $('a[href*="/user/register"], a[href*="/login"]').click(function(){
      $this = $(this);
      if(!$this.hasClass('is-processed')){
        return;
      }
      var link_href = $this.attr('href'),
        new_page = $this.attr('target') != '_blank' && link_href.indexOf('/penton_ur') == -1;
      var body = $('body');
      if(body.hasClass('node-type-forum')||body.hasClass('page-forum')){
        action='forum';
      }
      if(action!='forum'){
        if($this.hasClass('login-to-comment')||($this.parent().parent().hasClass('comment_forbidden')||$this.parent().hasClass('no-posting-permission'))){
          action='comment';
        }
      }
      if($this.closest('.gatedLoginButtons').length) {
        action = 'gate';
      }
      if($this.closest('.login-logout').length || $this.closest('.tablet-login').length) {
        action = 'nav';
      }
      if($this.hasClass('print-page')) {
        action = 'print';
      }
      if($this.hasClass('forward-page')) {
        action = 'email';
      }
      if($this.attr('href').indexOf('source=download') > -1) {
        action = 'download';
      }
      if(action!='' && new_page){
        registration_initiation_action(action, new_page);
      }
    });
    $('a[href*="/login"].user_follow_link_login').click(function () {
      $this=$(this);
      if(!$this.hasClass('is-processed')){
        return;
      }
      var new_page = $this.attr('target') != '_blank' && $this.attr('href').indexOf('/penton_ur') == -1;
      action='xchange';
      registration_initiation_action(action, new_page);
    });
    $('.add-to-cart, .node-add-to-cart, .list-add-to-cart').click(function () {
      action='ecomm';
      registration_initiation_action(action, true);
    });
    //Set newsletters for flat registration form
    var flat_form=$('form#user-register.flat-registration-page');
    if(flat_form.length){
      set_flat_newsletters();
      set_default_newsletter();
    }
    flat_form.find('fieldset .form-checkbox[id^="penton-registration-newsletter-checkbox"]').change(function(){
      $this=$(this);
      set_flat_newsletters();
      set_default_newsletter();
    });
    function set_flat_newsletters(){
      newsletters='';
      $.each(flat_form.find('fieldset .form-checkbox[id^="penton-registration-newsletter-checkbox"]:checked'),function(key, value){
        var title_div=$(this).closest('label').find('.enews-title');
        newsletters+=','+title_div.html();
      });
      newsletters=newsletters.substr(1);
    }

    function navigate(link_href) {
      window.location = link_href;
    }

    //Gallery tracking
    $('.related-articles .pm-related-gals li a').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event52';
      s.eVar33='related gallery';
      s.eVar69=s_account;
      s.events='event52';
      s.tl(this,'o','related gallery',null,navigate(link_href));
      return false;
    });

    $('body.node-type-gallery .gal-options .expand').click(function() {
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event51';
      s.eVar33='gallery expand';
      s.eVar69=s_account;
      s.events='event51';
      s.tl(this,'o','gallery expand');
    });

    //Share links below images in gallery
    var sharelinks_container = $('.pm-social-icons, .adaptive-sharelinks .social-links');
    sharelinks_container.find('.forward-page.ctools-use-modal').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      pm_gallery_share_track(s, 'page-emailed', 'event11');
      s.tl(this,'o','page-emailed');
    });

    sharelinks_container.find('.googleplusone-button a').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      pm_gallery_share_track(s, 'Google Plus One Click', 'event33');
      s.tl(this,'o','Google Plus One Click');
    });

    sharelinks_container.find('.twitter-button a').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      pm_gallery_share_track(s, 'Twitter Share', 'event39');
      s.tl(this,'o','Twitter Share');
    });

    sharelinks_container.find('.fb-button a').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      pm_gallery_share_track(s, 'fb:like', 'event35');
      s.tl(this,'o','fb:like');
    });

    sharelinks_container.find('.pinterest-button a').click(function() {
      var link_href = $(this).attr('href');
      var s = s_gi(s_account);
      pm_gallery_share_track(s, 'pinterest:pin it', 'event50');
      s.tl(this,'o','pinterest:pin it');
    });

    function pm_gallery_share_track(s, message, event){
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents=event;
      s.eVar33=message;
      s.eVar69=s_account;
      s.events=event;
    }
  });
});

function set_newsletters(){
  newsletters='';
  var checkboxes = $('form:not(#newsletter-registration-form)').find('.en-newsletter-listing .form-checkbox:checked');
  $.each(checkboxes,function(key, value){
    var title_div=$(this).closest('label').find('.enews-title');
    newsletters+=','+title_div.html();
  });
  newsletters=newsletters.substr(1);
}

function set_default_newsletter(){
  var checkbox = $('.news_checkbox .form-checkbox');
  if(!checkbox.length) {
    return;
  }
  if(!checkbox.attr('checked')) {
    newsletters = '';
  } else {
    var label = checkbox.closest('label'),
      html = label.html(),
      begin = html.indexOf('Sign up for') + 'Sign up for '.length,
      end = html.length - ' newsletter'.length,
      title = html.substring(begin, end);
    newsletters = title;
  }
}

function set_newsletters_cookie(){
  var extime = 3600;
  if(newsletters == '') {
    extime=0;
  }
  set_cookie('eVar[1]',newsletters,extime);
}

function set_username(input) {
  userName = input.val();
}

