    modal_form_loaded('full modal registration');
  });
  $('a[href="/penton_ur/nojs/light/register"]').click(function () {
    modal_form_loaded('light modal registration');
  });
  $('a[href="/user/register"]').click(function () {
    modal_form_loaded('regular registration');
  });
  */


  $('.en-newsletter-listing .form-checkbox').change(function(){
    $this=$(this);
    set_newsletters();
  });

  $('.news_checkbox .form-checkbox').change(function(){
    $this=$(this);
    set_default_newsletter();
  });

  var forms=$('#penton-registration-step1,#penton-registration-light-form,#penton-registration-light-for-email-form');

  forms.find('.form-item .form-text[name="name"]').change(function(){
    $this=$(this);
    set_username($this);
  });

  forms.find('.form-item .form-text[name="name"]').keyup(function(){
    $this=$(this);
    set_username($this);
  });

  //Newsletter flyover
  $('#penton-registration-newsletter-flyover-form').ajaxSuccess(function(e, xhr, settings) {
    $this=$(this);
    if($this.length){
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event47';
      s.eVar33='newsletter-flyout-nli';
      s.eVar69=s_account;
      s.events='event47';
      s.tl(this,'o','newsletter-flyout-nli');
