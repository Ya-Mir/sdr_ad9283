    }
    $this.unbind('ajaxSuccess');
  });

  $('#penton-user-registration-step1').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded(is_comment+'full modal registration');
  });
  $('#penton-user-registration-light-step1').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded(is_comment+'light modal registration');
  });
  $('#penton-user-registration-light-step2').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('light modal registration successful');
  });
  $('#penton-user-registration-step2').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('registration successful - profile');
  });
  $('#penton-user-registration-step-esp-web-services').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('registration successful - esp');
  });
  $('#penton-user-registration-step-hallmark-data-systems').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('registration successful - hallmark');
  });
  $('#penton-user-registration-step3').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('registration complete - newsletters');
  });
  $('#penton-user-registration-step4').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded('registration complete - 3rd party');
  });
  //Penton Simple Registration
  $('#penton-user-registration-tkli').ajaxSuccess(function(e, xhr, settings) {
    modal_form_loaded_simple('registration complete - 3rd party');
  });
  var newsletters_form = $('#penton-registration-newsletter-form');
  newsletters_form.ajaxSuccess(function(e, xhr, settings) {
    if($('#penton-registration-newsletter-form').length) {
      modal_form_loaded_simple('registration complete - newsletters');
      set_newsletters();
    }
  });
  newsletters_form.submit(function () {
    set_newsletters_cookie();
  });
  var full_modal_form = $('#penton-registration-step1');
  full_modal_form.ajaxSuccess(function(e, xhr, settings) {
    $this = $(this);
    if($this.length && !$this.find('.error').length) {
      modal_form_loaded_simple('full modal registration', true);
      set_default_newsletter();
    }
    $this.unbind('ajaxSuccess');
  });
  full_modal_form.submit(function () {
    //set_cookie('prop[46]', 'registration complete - thank you', 3600);
    //set_cookie('pageName', ':Thank You', 3600);
    set_cookie('frmclsd', 'yes', 3600);
    set_newsletters_cookie();
  });
  var simple_modal_forms = $('#penton-registration-light-form, #penton-registration-light-for-email-form');
  simple_modal_forms.ajaxSuccess(function(e, xhr, settings) {
    $this = $(this);
    if($this.length && !$this.find('.error').length) {
      modal_form_loaded_simple('simple modal registration', true);
      set_default_newsletter();
    }
    $this.unbind('ajaxSuccess');
  });
  simple_modal_forms.submit(function () {
    set_cookie('prop[46]', 'registration complete - thank you', 3600);
    set_cookie('pageName', ':Thank You', 3600);
    var events=get_cookie('events');
    if(events!=''){
        events+=',';
    }
    events+='event3';
    set_cookie('events',events,3600);
    set_newsletters_cookie();
  });
  //Login
  var login_forms = $('#user-login, #penton-registration-email-and-login-form');
  login_forms.ajaxSuccess(function(e, xhr, settings) {
    $this = $(this);
    if($this.length && !$this.find('.error').length && !$this.hasClass('.flat-login-page')) {
      if($this.hasClass('penton-registration-login-form')) {
        modal_form_loaded_simple('modal login', true);
      } else {
        modal_form_loaded(is_comment+'modal login');
      }
    }
    $this.unbind('ajaxSuccess');
  });
  //Password request
  var pass_form = $('#user-pass');
  pass_form.ajaxSuccess(function(e, xhr, settings) {
    $this = $(this);
    if($this.length && !$this.find('.error').length && !$this.hasClass('.flat-pass-page')) {
      if($(this).hasClass('penton-registration-pass-form')) {
        modal_form_loaded_simple('modal password request');
      } else {
        //modal_form_loaded(is_comment+'modal password request');
      }
    }
    $this.unbind('ajaxSuccess');
  });

  //No thanks, just print
  $('.no-thanks-print-link a').click(function() {
    set_cookie('prop[46]', 'skip reg - print', 3600);
  });

  $('.ctools-modal-content .modal-header a.close').click(function(){
    var modal = $(this).closest('.ctools-modal-content');
    if(modal.find('.penton-registration-login-form, .penton-registration-pass-form, ' +
      '#penton-registration-step1, #penton-registration-light-form, #penton-registration-light-for-email-form, .penton_registration_simple_thank_you').length)
    {
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event65';
      s.eVar33='form close';
      s.eVar69=s_account;
      s.events='event65';
      s.tl(this,'o','form close');
    }
    if(modal.find('#penton-registration-newsletter-flyover-form').length) {
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event47';
      s.eVar33='newsletter-flyout-close';
      s.eVar69=s_account;
      s.events='event47';
      s.tl(this,'o','newsletter-flyout-close');
    }
    if(modal.find('#penton-registration-subscribe-form').length) {
      var s = s_gi(s_account);
      s.linkTrackVars='eVar33,eVar69,events';
      s.linkTrackEvents='event47';
      s.eVar33='newsletter-flyout-close-nli';
      s.eVar69=s_account;
      s.events='event47';
      s.tl(this,'o','newsletter-flyout-close-nli');
    }
    $(this).unbind('click');
  });

  $('.block a:not(.non-omniture)').click(function() {
    var clicked_link = '',
        $this = $(this),
        link_href = $this.attr('href'),
        new_window = $this.attr('target'),
        $block = $this.closest('.block'),
        block_title = $.trim($block.find('.block-title').text()),
        block_id = $block.attr('id');
    if (!link_href || $this.filter('[disabled]').length) {
        return false;
    }
    if (block_title == '') {
      block_title = block_id;
    }
    if (block_id == 'block-nice_menus-1') {
      var primary_link = ':' + $.trim($this.closest('.menuparent').children('a').text()),
          this_link = ":" + $.trim($this.text());
      if (primary_link == this_link) {
        this_link = '';
      }
      clicked_link = 'Navigation' + primary_link + this_link;
    } else if ($this.parent().parent().hasClass('.quicktabs_tabs')) {
      return false;
    } else if ($this.parent().hasClass('.user-follow-link')) {
      return false;
    } else if ($this.parent().parent().hasClass('.logged-out')) {
      return true;
    } else if ($this.attr('rel') == 'author') {
      clicked_link = block_title + ' - author';
    } else if ($this.hasClass('category')) {
      clicked_link = block_title + ' - category';
    } else {
      clicked_link = block_title;
    }
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event47';
    s.eVar33=clicked_link;
    s.eVar69=s_account;
    s.events='event47';
    if (new_window == '_blank') {
      s.tl(this,'o',clicked_link);
    } else {
      s.tl(this,'o',clicked_link,null,navigate(link_href));
      return false;
    }
  });

/* linkedIn */
  $('.linkedin-button a').click(function(){
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event37';
    s.eVar33='LinkedIn Share';
    s.eVar69=s_account;
    s.events='event37';
    s.tl(this,'o','LinkedIn Share');
    $(this).unbind('click');
  });

/* favorite - only the bottom of content */
  $('.fav-link').click(function(){
    var link_href = $(this).attr('href');
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event59';
    s.eVar33='page-favorited'; // should provide a unique favorite value
    s.eVar69=s_account;
    s.events='event59';
    s.tl(this,'o','page-favorited',null,navigate(link_href));
    return false;
  });
/**/

/* email - top and bottom of content */
  $('.forward-page:not(.non-omniture)').click(function(){
    if($(this).attr('href').indexOf('/user/register')>-1){
      if($(this).attr('href').indexOf('/penton_ur')==-1){
        var events=get_cookie('events');
        if(events!=''){
          events+=',';
        }
        events+='event11';
        set_cookie('events',events,3600);
      }
      return;
    }
    var link_href = $(this).attr('href');
    var s = s_gi(s_account);
    forwardPage(s);
    s.tl(this,'o','page-emailed',null,navigate(link_href));
    return false;
  });
  $('.forward-page.non-omniture').click(function(){
    var s = s_gi(s_account);
    forwardPage(s);
  });
  function forwardPage(s) {
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event11';
    s.eVar33='page-emailed'; // should provide a unique email value
    s.eVar69=s_account;
    s.events='event11';
  }
/**/

/* print - only the bottom of content */
  $('.print-page').click(function(){
    if($(this).attr('href').indexOf('/user/register')>-1){
      return;
    }
    var s = s_gi(s_account);
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event12';
    s.eVar33='page-printed'; // should provide a unique print value
    s.eVar69=s_account;
    s.events='event12';
    s.tl(this,'o','page-printed');
  });
/**/

/* inline related or popular now content (manually added to the body content) */
  $('.related-content a').click(function(){
    var $this = $(this),
        link_href = $this.attr('href'),
        new_window = $this.attr('target'),
        s = s_gi(s_account),
        message = 'inline-related';
    if($this.parent().parent().hasClass('popular-now')) {
      message = 'chartbeat-trending';
    }
    s.linkTrackVars='eVar33,eVar69,events';
    s.linkTrackEvents='event47';
    s.eVar33=message; // should provide a unique related or popular now value
    s.eVar69=s_account;
    s.events='event47';
    if (new_window == '_blank') {
      s.tl(this,'o',message);
    } else {
      s.tl(this,'o',message,null,navigate(link_href));
      return false;
    }
  });
/**/

  function navigate(link_href) {
    window.location = link_href;
  }
};


/* Initiate doPlugins Function */
s.usePlugins=true;
function s_doPlugins(s) {

/* Add calls to plugins here */

/* Set Campaign Variables */
  s.campaign=s.getQueryParam('pid,ym_mid,utm_campaign',':');
  s.eVar45=s.getQueryParam('nl,utm_source',':');
  s.eVar68=s.getQueryParam('pk,code,promo',':');

/* Capture Domain */
  var domain = window.location.hostname;
  if(domain.indexOf('www.')==0)
    domain = domain.substring(4);
  s.prop2=domain;

  /* get slide number for galleries */
  if (s.prop6 == 'gallery') {
    var slide = window.location.hash.match(/[0-9]+/g),
        slides = $('.pm-gal-slide','.pm-gal-wrapper').size();
    if (slide instanceof Array) {
      slide = parseInt(slide[0],10) + 1;
    } else {
      slide = 1;
    }
    s.prop8 = s.prop8 ? s.prop8 : 'Slide ' + slide + ' of ' + slides;
  }
/* Plugin Example: getDaysSinceLastVisit v1.1 */
  var so_daysSince=s.getDaysSinceLastVisit('s_lv');
  s.prop23=s.getValOnce(so_daysSince,'s.daysSince',0);

/* Track gated content abandonment */
  s.eVar46="D=prop46";
  if (s.prop46=='wall') {
    s.eVar49="D=pageName";
    s.events=s.apl(s.events,'event4',',',1);
  }
  if (s.prop46=='verify') {
    s.events=s.apl(s.events,'event5',',',1);
  }

  s.eVar16="D=s_vi"; // passes Visitor ID

/* Demandbase Code Goes Here */
  s.eVar51=company_name; s.eVar53=employee_count; s.eVar54=primary_sic; s.eVar56=company_size;

/* Copy Page Name for Standardization Purposes */
  s.prop58="D=pageName";

/* UID Tracking */
  s.eVar59=s.getQueryParam('utm_rid');

/* YM NL Position Track */
  s.eVar28=s.getQueryParam('cl');

/* Set Once Per Visit Event */
  s.visEvent=s.getVisitStart("s_visit");
  if (s.visEvent) s.events=s.apl(s.events,'event18',',',1);

/* Set Page Views Consumed Event */
  s.events=s.apl(s.events,'event1',',',1);

/* Advanced Download Tracking */
  var url=s.linkHandler('.exe|.zip|.wav|.mp3|.mov|.mpg|.avi|.wmv|.doc|.pdf|.xls','d');
  if(url){
    // set variables (props,evars,linkTrackVars,linkTrackEvents)
    url = url.substring(url.lastIndexOf('/')+1);
    s.linkTrackVars="prop32,prop58,prop63,eVar63,eVar69,events";
    s.linkTrackEvents="event22";
    if (s.prop29) {
      s.prop32 = s.prop29 + ": Download: " + url;
    }
    s.prop63 = url;
    s.eVar63 = "D=c63";
    s.prop58 = "Download: " + url;
    s.eVar69=s_account;
    s.events = s.events?s.events + ",event22":"event22";
  }

/* Advanced Exit Link Tracking */
  var url2=s.getDestinationURL();
  if(url2){
    if(s.inFilts(url2)==-1){
      s.linkTrackVars="prop32,prop58,prop62,eVar62,eVar69,events";
      s.linkTrackEvents="event21";
      if (s.prop29) {
        s.prop32 = s.prop29 + ": Exit: " + url2;
      }
      s.prop62 = url2;
      s.eVar62 = "D=c62";
      s.prop58 = "Exit: " + url2;
      s.eVar69=s_account;
      s.events = s.events?s.events + ",event21":"event21";
      s.linkType='e';
    }
  }

/* Traffic Driver Type */
  if(s.ActionDepthTest){
    if(s.getActionDepth("s_depth") == 1){
      s.channelManager('cid,eid,pid,mid,iid,YM_MID,code',':','s_cm','','s_dirL');
      if(document.referrer){
        if(s.inFilts(document.referrer)==1){
          s._channel = "Inside Site";
        }
      }
      if(s.getQueryParam("utm_source").indexOf("twitter")>-1){
        s._channel = "Social Media";
      }
      if(s._channel=="Referrers"){
        s._channel="Other Websites";
      }
      if(s._channel=="Direct Load"){
        s._channel="Direct Traffic";
      }
      s.prop52=s._channel;
    } else {
      s.prop52='Internal';
      if(s.getActionDepth("s_depth")==3){
        s.events=s.apl(s.events,"event32",",",2);
      }
    }
    s.ActionDepthTest=false;
  }
  s.prop53 = s.getAndPersistValue(s._channel,'gpb_tdt',0);

/* Get New/Repeat Visit - 365 Day Cookie */
  s.eVar65=s.getNewRepeat(365,'s_getNewRepeat');

/* Get Visit Number */
  s.eVar66=s.getVisitNum();

/* Capture Account ID for Global Report */
  s.eVar69=s_account;

/* Pass products variables to eVars for longer attribution
  if(s.products) {
    s.eVar7=s.products.split(";", 1);
    s.eVar8=s.products.slice(s.products.indexOf(';')+1);
  } else {
    s.eVar7=s.eVar8="Not Defined";
  }
*/
  s.eVar11=s.getTimeParting('d','-5')+" > "+s.getTimeParting('h','-5');

/* Variable Equations */
  s.prop24 = s.getPreviousValue(s.pageName, 'gpv_pagnam', '');  // get previous value for s.pageName
  s.prop38 = s.getPreviousValue(s.prop3, 'gpv_intsea', '');  // get previous value for internal search

}

/* User Status Tracking */
if(scData.reg == 'true'){
  if(scData.ver == 'true') {
    s.eVar15 = 'reg_ver_nli';
  } else {
    s.eVar15 = 'reg_unver_nli';
  }
} else {
  s.eVar15 = 'unreg';
}

