_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?)(:\d*)?)(\/((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)+(\/(([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)*)*)?)?(\?((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|[\uE000-\uF8FF]|\/|\?)*)?(#((([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(%[\da-f]{2})|[!\$&'\(\)\*\+,;=]|:|@)|\/|\?)*)?$/i.test(a)},date:function(a,b){return this.optional(b)||!/Invalid|NaN/.test(new Date(a).toString())},dateISO:function(a,b){return this.optional(b)||/^\d{4}[\/\-](0?[1-9]|1[012])[\/\-](0?[1-9]|[12][0-9]|3[01])$/.test(a)},number:function(a,b){return this.optional(b)||/^-?(?:\d+|\d{1,3}(?:,\d{3})+)?(?:\.\d+)?$/.test(a)},digits:function(a,b){return this.optional(b)||/^\d+$/.test(a)},creditcard:function(a,b){if(this.optional(b))return"dependency-mismatch";if(/[^0-9 \-]+/.test(a))return!1;var c,d,e=0,f=0,g=!1;if(a=a.replace(/\D/g,""),a.length<13||a.length>19)return!1;for(c=a.length-1;c>=0;c--)d=a.charAt(c),f=parseInt(d,10),g&&(f*=2)>9&&(f-=9),e+=f,g=!g;return e%10===0},minlength:function(b,c,d){var e=a.isArray(b)?b.length:this.getLength(a.trim(b),c);return this.optional(c)||e>=d},maxlength:function(b,c,d){var e=a.isArray(b)?b.length:this.getLength(a.trim(b),c);return this.optional(c)||d>=e},rangelength:function(b,c,d){var e=a.isArray(b)?b.length:this.getLength(a.trim(b),c);return this.optional(c)||e>=d[0]&&e<=d[1]},min:function(a,b,c){return this.optional(b)||a>=c},max:function(a,b,c){return this.optional(b)||c>=a},range:function(a,b,c){return this.optional(b)||a>=c[0]&&a<=c[1]},equalTo:function(b,c,d){var e=a(d);return this.settings.onfocusout&&e.unbind(".validate-equalTo").bind("blur.validate-equalTo",function(){a(c).valid()}),b===e.val()},remote:function(b,c,d){if(this.optional(c))return"dependency-mismatch";var e,f,g=this.previousValue(c);return this.settings.messages[c.name]||(this.settings.messages[c.name]={}),g.originalMessage=this.settings.messages[c.name].remote,this.settings.messages[c.name].remote=g.message,d="string"==typeof d&&{url:d}||d,g.old===b?g.valid:(g.old=b,e=this,this.startRequest(c),f={},f[c.name]=b,a.ajax(a.extend(!0,{url:d,mode:"abort",port:"validate"+c.name,dataType:"json",data:f,context:e.currentForm,success:function(d){var f,h,i,j=d===!0||"true"===d;e.settings.messages[c.name].remote=g.originalMessage,j?(i=e.formSubmitted,e.prepareElement(c),e.formSubmitted=i,e.successList.push(c),delete e.invalid[c.name],e.showErrors()):(f={},h=d||e.defaultMessage(c,"remote"),f[c.name]=g.message=a.isFunction(h)?h(b):h,e.invalid[c.name]=!0,e.showErrors(f)),g.valid=j,e.stopRequest(c,j)}},d)),"pending")}}}),a.format=function(){throw"$.format has been deprecated. Please use $.validator.format instead."};var b,c={};a.ajaxPrefilter?a.ajaxPrefilter(function(a,b,d){var e=a.port;"abort"===a.mode&&(c[e]&&c[e].abort(),c[e]=d)}):(b=a.ajax,a.ajax=function(d){var e=("mode"in d?d:a.ajaxSettings).mode,f=("port"in d?d:a.ajaxSettings).port;return"abort"===e?(c[f]&&c[f].abort(),c[f]=b.apply(this,arguments),c[f]):b.apply(this,arguments)}),a.extend(a.fn,{validateDelegate:function(b,c,d){return this.bind(c,function(c){var e=a(c.target);return e.is(b)?d.apply(e,arguments):void 0})}})});;/**/
(function($) {
  // put together the name of the new jQuery variable
  var version = $().jquery;
  var versionName = version.replace(/\./g, '');
  // save the new version of jQuery globally and restore Drupal version
  window['jq' + versionName] = jQuery.noConflict(true);
})(jQuery);;/**/
/**
 * Handle newsletter signup related JS
 */
Drupal.behaviors.newsletter_registration = function(context) {
  if (context === document) {

    // Prevent Enter key from submitting form
    $("#newsletter_node_submit_form input").keypress(function(e) {
      if (event.keyCode === 10 || event.keyCode === 13) {
        event.preventDefault();
      }
    });


    // Submit handler for ALL newsletter signup forms
    (function($) {
      if (typeof $ === 'undefined') { return false; }
      var elCountry = $("select[name=country]"),
        elLegalCanada = $("input[name=legal_canada]"),
        updateStateCanada;
      updateStateCanada = function(e) {
        var $this = $(this),
          elForm = $this.closest('form'),
          elCountry = elForm.find("select[name=country]"),
          elLegalCanada = elForm.find("input[name=legal_canada]"),
          button = elForm.find("[name=nl_submit]"),
          country = (elCountry.length ? elCountry.val() : '').toLowerCase();

        if (country === 'canada' && !elLegalCanada.is(':checked')) {
          button.attr('disabled', 'disabled');
        } else {
          button.removeAttr('disabled');
        }
      };
      elCountry.on('change', updateStateCanada);
      elLegalCanada.on('click', updateStateCanada);
    }(window.jq183));


    $("[name=nl_submit]").click(function(e) {
      e.preventDefault();
      var $this = $(this),
        elForm = $this.closest("form"),
        elEmail = elForm.find("input[name=email]"),
        elCountry = elForm.find("select[name=country]"),
        elLegalCanada = elForm.find("input[name=legal_canada]"),
        elNewsletters = elForm.find("input[type=checkbox]").not(elLegalCanada),
        elNewslettersChecked = elNewsletters.filter(':checked'),
        email = elEmail.length ? elEmail.val() : '',
        country = elCountry.length ? elCountry.val() : '',
        campaign = elForm.find("input[name=campaign]").val(),
        source = elForm.find("input[name=source]").val(),
        elContent = $(".pm-content"),
        newsletters = [],
        newsletter_names = [],
        newsletter_label = "",
        invalid_email = false,
        signupArea = $('.enews-signup'),
        validation_email_message='<div id="console" class="clearfix"><div class="messages error"><ul><li>Please enter a valid email address.</li>';

      if ($this.filter('[disabled]').length) {
        return false;
      }

      // Check for valid email. If invalid do JS equiv of DSM
      if (!validateEmail(email)) {
        $("#console").remove();
        elContent.prepend(validation_email_message + '</ul></div></div>');
        elEmail.addClass('newsletter_error');
        elLegalCanada.removeClass('newsletter_error');
        $('html, body').animate({ scrollTop: 0 }, 'slow');
        invalid_email = true;
      }
      // Check for select checkbox about Canada. If invalid do JS equiv of DSM
      if (country.toLowerCase() === 'canada' && !elLegalCanada.is(':checked')) {
        $("#console").remove();
        if (!invalid_email) {
          elContent.prepend('<div id="console" class="clearfix"><div class="messages error"><li>Canadian users must acknowledge their acceptance of our terms of service and privacy policy.</li></ul></div></div>');
          elEmail.removeClass('newsletter_error');
        } else {
          elContent.prepend(validation_email_message + '<li>Canadian users must acknowledge their acceptance of our terms of service and privacy policy.</li></ul></div></div>');
        }
        elLegalCanada.addClass('newsletter_error');
        $('html, body').animate({ scrollTop: 0 }, 'slow');
        return;
      }

      if (invalid_email) { return; }

      elEmail.removeClass('newsletter_error');
      elLegalCanada.removeClass('newsletter_error');
      // Find what kind of form submission is called from
      // If we are on a page with checkboxes and some are selected...
      if(elNewslettersChecked.length){
        $("#console").remove();
        elNewslettersChecked.each(function(i) {
          newsletters.push({name: i, value: $(this).parents(".en-newsletter-listing").children(".newsletter-nid").attr("value")});
          newsletter_label = $(this).parent().text();
          newsletter_names.push(newsletter_label);
        });
      }

      // If there are checkboxes and none are selected
      else if (elNewsletters.length && !elNewslettersChecked.length){
        $("#console").remove();
        elContent.prepend('<div id="console" class="clearfix"><div class="messages error">You must select at least one newsletter.</div></div>');
        elEmail.removeClass('newsletter_error');
        $('html, body').animate({ scrollTop: 0 }, 'slow');
        return;
      }
      // If it made it here they are on a single newsletter page
      else{
        $("#console").remove();
        newsletters.push({name: 0, value: elForm.find("input[name*=newsletter_info]").attr("value")});
        newsletter_label = $(".page-title").text();
        newsletter_names.push(newsletter_label);
      }

      //set super cookie
      setSC(email);

      // If we made it this far, should have a newsletter array, but
      // check in case
      if (newsletters.length) {
        $this.replaceWith('<img src="/sites/all/modules/custom/penton_user_registration/images/loading.gif"><p> Please wait...</p>');

        $.post("/newsletter/register/callback", {email: email, country: country, newsletters: $.param(newsletters), campaign: campaign, source: source}, function(data) {
          var success = data.indexOf('error') === -1, cbox_inline_html;
          // If successful post and record is set (unverified ATM)
          if (success) {
              // Setup Colorbox
            cbox_inline_html = [
              '<div class="colorbox-html">',
              '<div id="nl-thanks">Thank you for subscribing!</div><div id="nl-thanks-sub">You are now subscribed to the following newsletter(s):</div><div id="nl-thanks-listing">',
              '<ul>'
            ].join('');
            $.each(newsletter_names, function(index, value){
              if (value.indexOf(' - (') > 1) {
                value = value.split(' - (')[0];
              }
              cbox_inline_html += "<li>" + value + "</li>";
              //alert("index: " + index + " value: " + value);
            });
            cbox_inline_html += "</ul>";
            cbox_inline_html += '</div><div id="nl-thanks-suffix">Your subscription will begin with the next mailing.</div>';

            var path = window.location.pathname;
            if (path !== "/newsletters/signup") {
              cbox_inline_html += '<div id="all-nl-redirect"><a href="#" onclick="return false;" type="submit">Click here</a> to view all newsletters.</div>';
            }

            cbox_inline_html += '<div id="nl-thanks-close" class="button-region"><a href="#" class="btn btn-branded" onclick="return false;" name="nl-close" value="nl-close">Done!</a></div>';
            cbox_inline_html += '</div>';
            setupColorbox(cbox_inline_html);
            signupArea.css("display", "none");
            signupArea.after("<div class='subscription-success-title'><p>Thank you for subscribing!</p></div>");
          } else {
            // Error handling can go here
            cbox_inline_html = "There was an error processing your newsletter subscriptions! ";
            setupColorbox(cbox_inline_html);
          }
        }); // END $.post
      } // END if
    }); // END Submit handler
  }
  $("#nl-thanks-close").click(function(e){
    //e.preventDefault();
    $("#cboxClose").trigger('click');
  });

  $("#all-nl-redirect").click(function(){
    window.location = '/newsletters/signup';
  });
};

function validateEmail(email) {
  var re = /(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|"(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21\x23-\x5b\x5d-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])*")@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\[(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?|[a-z0-9-]*[a-z0-9]:(?:[\x01-\x08\x0b\x0c\x0e-\x1f\x21-\x5a\x53-\x7f]|\\[\x01-\x09\x0b\x0c\x0e-\x7f])+)\])/;
  return re.test(email);
}

/**
 * Setup Colorbox and invoke w/o clicking a Colorbox link
 *
 * @param cbox_inline_html
 *            Content to be displayed inside the Popup
 */
// Setup ColorBox
function setupColorbox(cbox_inline_html) {
  // Selector for the <a> element
  $("#inline_content").html(cbox_inline_html);
  //$(".inline").colorbox({inline:true, close:"Close"});
  //$(".inline").trigger('click');
  $.colorbox({
    html:cbox_inline_html,
    close: "Close",
    onOpen:function(){ $('#colorbox').addClass('newsletter-colorbox'); },
    onClosed:function(){ $('#colorbox').removeClass('newsletter-colorbox'); }
  });
};/**/
/**
 * Cookie plugin
 *
 * Copyright (c) 2006 Klaus Hartl (stilbuero.de)
 * Dual licensed under the MIT and GPL licenses:
 * http://www.opensource.org/licenses/mit-license.php
 * http://www.gnu.org/licenses/gpl.html
 *
 */
jQuery.cookie=function(B,I,L){if(typeof I!="undefined"){L=L||{};if(I===null){I="";L.expires=-1}var E="";if(L.expires&&(typeof L.expires=="number"||L.expires.toUTCString)){var F;if(typeof L.expires=="number"){F=new Date();F.setTime(F.getTime()+(L.expires*24*60*60*1000))}else{F=L.expires}E="; expires="+F.toUTCString()}var K=L.path?"; path="+(L.path):"";var G=L.domain?"; domain="+(L.domain):"";var A=L.secure?"; secure":"";document.cookie=[B,"=",encodeURIComponent(I),E,K,G,A].join("")}else{var D=null;if(document.cookie&&document.cookie!=""){var J=document.cookie.split(";");for(var H=0;H<J.length;H++){var C=jQuery.trim(J[H]);if(C.substring(0,B.length+1)==(B+"=")){D=decodeURIComponent(C.substring(B.length+1));break}}}return D}};
;/**/
